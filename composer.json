{
    "name": "microoculus/all-uploads",
    "description": "all-uploads for manage all uplods files.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "spatie/laravel-medialibrary": "^10.0.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Microoculus\\AllUploads\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Microoculus\\AllUploads\\Providers\\AllUploadsServiceProvider"
            ],
            "aliases": {
                "AllUploads": "Microoculus\\AllUploads\\Facades\\AllUploads"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Microoculus",
            "email": "microoculus@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
